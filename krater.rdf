        <otv:language>en</otv:language>
        <otv:denotedConcept rdf:resource="#Vessel_for_mixing_wine_with_water_with_handles_with_foot_with_open_mouth_without_neck_with_upward_curling_handles_upward_curling_handles_placed_low_on_the_body"/>
