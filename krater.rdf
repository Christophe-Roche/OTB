        <skos:narrower rdf:resource="#Vessel_for_storing_and_transport_with_neck_with_handles_with_two_handles_liquid_or_solid_with_foot_without_a_clearly_defined_neck_with_standard_belly_with_straight_lip_with_flat_handles"/>
